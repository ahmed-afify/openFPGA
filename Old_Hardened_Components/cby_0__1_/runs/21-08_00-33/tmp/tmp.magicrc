    addpath /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef/sky130_fd_sc_hvl
    addpath /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef/sky130_fd_sc_lp
    addpath /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef/sky130_fd_sc_ls
