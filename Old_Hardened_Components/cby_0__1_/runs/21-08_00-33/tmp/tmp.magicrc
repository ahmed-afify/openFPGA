puts stdout "Sourcing design .magicrc for technology sky130A ..."

# Put grid on 0.005 pitch.  This is important, as some commands don't
# rescale the grid automatically (such as lef read?).

set scalefac [tech lambda]
if {[lindex  1] < 2} {
    scalegrid 1 2
}

# drc off
drc euclidean on

# Allow override of PDK path from environment variable PDKPATH
if {[catch {set PDKPATH $::env(PDKPATH)}]} {
    set PDKPATH "/home/aa/Desktop/Afify/openlane/pdks/sky130A"
}

# loading technology
tech load /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.tech/magic/current/sky130A.tech

# load device generator
source /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.tech/magic/current/sky130A.tcl

# load bind keys (optional)
# source /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.tech/magic/current/sky130A-BindKeys

# set units to lambda grid 
snap lambda

# set sky130 standard power, ground, and substrate names
set VDD VPWR
set GND VGND
set SUB VSUBS

# Allow override of type of magic library views used, "mag" or "maglef",
# from environment variable MAGTYPE

if {[catch {set MAGTYPE $::env(MAGTYPE)}]} {
   set MAGTYPE maglef
}

# add path to reference cells
if {[file isdir /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef]} {
    addpath /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef/sky130_fd_pr_base
    addpath /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef/sky130_fd_pr_rf
    addpath /home/aa/Desktop/Afify/openlane/pdks/sky130A//libs.ref/maglef/sky130_fd_pr_rf2
